{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "dev setup",
			"type": "node",
			"request": "launch",
			"program": "dist/e5r-dev.js",
			"stopOnEntry": false,
			"args": [
                "setup",
                "--shell=powershell"
            ],
			"cwd": ".",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
                "NODE_DEBUG": "E5RDEV"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}